QPushButton[objectName^="menuButton"]
{
background-color: transparent;
border: none;
}

QWidget[objectName^="sideMenu"]
{
   background-color: rgb(255, 255, 255);

}


QWidget[objectName^="background_widget_url"],
QWidget[objectName^="background_widget_dropfile"],
QWidget[objectName^="background_widget_pkcs"],
QWidget[objectName^="background_widget_hardware"]
{
   border-radius: 30px;
   background-color: rgba(140, 118, 169, 255);
}

QWidget[objectName^="titleBar_widget"]
{
    background-color:rgb(9, 56, 94);
}


